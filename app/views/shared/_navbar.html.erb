<%# navbar %>
<div class="navbar navbar-expand-sm navbar-light navbar-lewagon">
  <a class="navbar-brand" href="#">
    <img src="https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/logo.png" />
  </a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <li class="nav-item">
        <a class="nav-link" id="navbar-links" href="#">Home</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" id="navbar-links" href="#">Cars</a>
      </li>
      <li class="nav-item dropdown">
        <img id="navbar-links" class="avatar dropdown-toggle" id="navbarDropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" src="https://media.istockphoto.com/vectors/user-icon-flat-isolated-on-white-background-user-symbol-vector-vector-id1300845620?b=1&k=20&m=1300845620&s=170667a&w=0&h=JbOeyFgAc6-3jmptv6mzXpGcAd_8xqkQa_oUK2viFr8=" />
        <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
        <% if user_signed_in? %>
        <%= link_to "Sign-out?",
            destroy_user_session_path,
            method: :delete,
            class: "dropdown-item",
            data: { confirm: "Are you sure?" } %>
        <% else %>
        <%= link_to "Inscription", new_user_registration_path, class:"dropdown-item" %>
        <%= link_to "Connexion", new_user_session_path, class:"dropdown-item" %>
        <% end %>
        </div>
      </li>
    </ul>
  </div>
</div>
